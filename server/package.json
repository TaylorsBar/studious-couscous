{
  "name": "karapiro-cartel-server",
  "version": "1.0.0",
  "description": "Karapiro Cartel Backend API - Automotive ecosystem server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "db:setup": "npm run db:generate && npm run db:push && npm run db:seed",
    "migration:create": "prisma migrate dev --create-only",
    "migration:run": "prisma migrate deploy",
    "blockchain:setup": "tsx src/blockchain/setup.ts",
    "blockchain:deploy": "tsx src/blockchain/deploy.ts",
    "crm:sync": "tsx src/services/crm/sync.ts",
    "financial:sync": "tsx src/services/financial/sync.ts",
    "compliance:check": "tsx src/services/compliance/check.ts"
  },
  "dependencies": {
